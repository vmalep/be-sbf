{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "decimal"
    },
    "library": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::library.library",
      "inversedBy": "books"
    },
    "is_available": {
      "type": "boolean"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "books"
    },
    "school_year_id": {
      "type": "string"
    },
    "comment": {
      "type": "richtext"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "asNew",
        "fine",
        "veryGood",
        "good",
        "fair",
        "poor",
        "bindingCopy"
      ]
    },
    "img_url": {
      "type": "string"
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "books"
    },
    "school_year": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::school-year.school-year",
      "inversedBy": "books"
    },
    "reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "book"
    }
  }
}
